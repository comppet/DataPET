<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:comp="http://java.sun.com/jsf/composite/comp">
    <h:body>
        <!-- esta página utiliza como template o tema "template.xhtml" -->
        <ui:composition template="template.xhtml">
            <!-- O titulo desta página é "página inicial" -->
            <ui:param name="titulo" value="documentação" />
            <!-- O que foi informado como "conteúdo" no template deve ser preenchido com isto: -->
            <ui:define name="conteudo">
                <div class="titulo">
                    <img src="imgs_layout/img_documentacao.gif" width="65" height="60" alt="Documentação" class="titulo_img" />
                    <h1>Documentação</h1>
                    <span>Confira <a href="http://www.comppet.ufu.br/datapet1/documentacao/javadoc/index.html" target="blank">aqui</a> documentação do DataPET para melhor entender seu funcionamento.</span>
                </div>
                
                <p></p>
            </ui:define>
        </ui:composition>
    </h:body>
</html>