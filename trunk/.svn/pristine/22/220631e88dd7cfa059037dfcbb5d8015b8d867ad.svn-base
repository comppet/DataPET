package Model;

/**
 *
 * @author Tiago Peres
 */
public class Curso {
    // tipos de formação
    public static final int BACHARELADO = 0;
    public static final int LICENCIATURA = 1;
    public static final int BACHARELADOLICENCIATURA = 2;

    // períodos possíveis
    public static final int SEMESTRAL = 1;
    public static final int ANUAL = 2;

    // atributos de instância
    private int id;
    private Instituicao instituicao;
    private String nome;
    private int formacao;
    private int periodo;

    /**
     * @return the id
     */
    public int getId() {
        return id;
    }

    /**
     * @param id the id to set
     */
    public void setId(int id) {
        this.id = id;
    }

    /**
     * @return the nome
     */
    public String getNome() {
        return nome;
    }

    /**
     * @param nome the nome to set
     */
    public void setNome(String nome) {
        this.nome = nome;
    }

    /**
     * @return the formacao
     */
    public int getFormacao() {
        return formacao;
    }

    /**
     * @param formacao the formacao to set
     */
    public void setFormacao(int formacao) {
        this.formacao = formacao;
    }

    /**
     * @return the periodo
     */
    public int getPeriodo() {
        return periodo;
    }

    /**
     * @param periodo the periodo to set
     */
    public void setPeriodo(int periodo) {
        this.periodo = periodo;
    }

    /**
     * @return the instituicao
     */
    public Instituicao getInstituicao() {
        return instituicao;
    }

    /**
     * @param instituicao the instituicao to set
     */
    public void setInstituicao(Instituicao instituicao) {
        this.instituicao = instituicao;
    }
}
